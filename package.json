{
  "name": "@aureooms/js-sll",
  "description": "Singly linked list code bricks for JavaScript",
  "version": "1.0.2",
  "author": "aureooms",
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-async-generator-functions"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-sll/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-array": "4.0.0",
    "@aureooms/js-itertools": "4.0.0",
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.0",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/register": "7.10.5",
    "ava": "3.11.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.3.2",
    "nyc": "15.1.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "http://aureooms.github.io/js-sll/",
  "keywords": [
    "bricks",
    "ender",
    "javascript",
    "js",
    "linked",
    "linked list",
    "list",
    "lists",
    "node",
    "nodes",
    "singly",
    "singly linked list",
    "sll"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-sll.git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  }
}
